<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wx.manage.mapper.SystemDataSourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.wx.manage.pojo.entity.SystemDataSource">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="ip" property="ip" />
        <result column="port" property="port" />
        <result column="url" property="url" />
        <result column="username" property="username" />
        <result column="password" property="password" />
        <result column="sort" property="sort" />
        <result column="type" property="type" />
        <result column="is_deleted" property="isDeleted" />
        <result column="version" property="version" />
        <result column="creator" property="creator" />
        <result column="create_time" property="createTime" />
        <result column="updater" property="updater" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, ip, port, url, username, password, sort, type, is_deleted, version, creator, create_time, updater, update_time
    </sql>

</mapper>
